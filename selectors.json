{
  "version": "2.1",
  "lastUpdated": "2025-05-23T10:30:00Z",
  "description": "Dynamic selectors for Claude.ai interface detection",
  "selectors": {
    "generation": {
      "stopButton": [
        "[data-testid='stop-button']",
        "button[aria-label*='Stop']",
        "button:has(svg[data-icon='stop'])",
        "[data-testid='stop-generation-button']",
        "button[data-state='generating']",
        ".stop-generation-btn",
        "button[title*='Stop']",
        "[aria-label='Stop generating']"
      ],
      "loading": [
        "[data-testid='loading']",
        ".loading",
        "[class*='loading']",
        "[class*='generating']",
        "[data-testid='message-loading']",
        ".message-streaming",
        "[data-state='loading']",
        ".animate-pulse",
        "[data-testid='thinking']"
      ]
    },
    "error": [
      "[data-testid='error']",
      ".error",
      "[class*='error']",
      "[role='alert']",
      ".text-red-500",
      ".text-red-600",
      ".text-red-700",
      "[data-testid='error-message']",
      ".error-banner",
      "[data-state='error']",
      ".alert-error",
      "[class*='text-red']"
    ]
  },
  "metadata": {
    "supportedVersions": ["claude-3", "claude-4"],
    "contributors": ["community"],
    "nextUpdate": "2025-05-30"
  }
}